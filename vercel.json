{
    "version": 2,
    "builds": [{
        "src": "Event/wsgi.py",
        "use": "@vercel/python"
    }],
    "routes": [{
            "src": "/static/(.*)",
            "dest": "/staticfiles/$1"
        },
        {
            "src": "/media/(.*)",
            "dest": "/media/$1"
        }
    ]
}